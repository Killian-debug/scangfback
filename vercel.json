{
    "version": 2.1,
    "name" : "scangfback",
    "builds" : [
        {"src" : "server.js", "use": "@vercel/node"}
    ],
    "routes": [
        { "src": "/(.*)", "dest": "server.js" }
    ],
    "functions": {
        "router/*.js": {
        "memory": 3008,
        "maxDuration": 30
        }
    }

}